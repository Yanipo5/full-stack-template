{
  "name": "full-stack-server",
  "version": "0.0.1",
  "engines": {
    "node": ">= 16 <17"
  },
  "scripts": {
    "dev": "SET NODE_ENV=development&& nodemon -e ts -w ./src -x ts-node --pretty ./src/server.ts",
    "start": "NODE_ENV=development ts-node --pretty ./src/server.ts",
    "clean": "rimraf coverage build tmp",
    "build": "npx tsc",
    "production": "NODE_ENV=production node dist/server.js",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "generate-prisma": "npx prisma generate",
    "format-prisma": "npx prisma format",
    "push-prisma": "npx prisma db push",
    "pull-prisma": "npx prisma db pull",
    "init-prisma": "npx prisma migrate dev --name init",
    "studio-prisma": "npx prisma studio",
    "install-production": "npm ci",
    "install-dev": "npm install && npm run generate-prisma"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "@trpc/server": "9.25.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.12.1",
    "lodash": "^4.17.21",
    "prisma": "4.0.0",
    "superjson": "^1.9.1",
    "tslib": "2.4.0",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "17.0",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "6.9.7",
    "full-stack-prisma-trpc-vue-element-plus": "file:..",
    "jest": "28.1.2",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.2",
    "tsutils": "3.21",
    "typescript": "4.7.4"
  },
  "author": "Daniel Yogel",
  "license": "MIT"
}
