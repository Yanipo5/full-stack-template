{
  "name": "full-stack-server",
  "version": "0.0.0",
  "main": "dist/server.js",
  "scripts": {
    "_____DEV____": "",
    "dev": "SET NODE_ENV=development&& npx nodemon -e ts -w ./src -x ts-node --pretty ./src/server.ts",
    "_____BUILD____": "",
    "build": "npx tsc",
    "postinstall": "npx prisma generate",
    "typecheck": "tsc --noEmit",
    "_____TEST____": "",
    "_____PRISMA____": "",
    "generate-prisma": "npx prisma generate",
    "format-prisma": "npx prisma format",
    "push-prisma": "npx prisma db push",
    "pull-prisma": "npx prisma db pull",
    "init-prisma": "npx prisma migrate dev --name init",
    "studio-prisma": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "@trpc/server": "9.25.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.12.1",
    "lodash": "^4.17.21",
    "prisma": "4.0.0",
    "superjson": "^1.9.1",
    "tslib": "2.4.0",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "17.0",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "6.9.7",
    "full-stack-prisma-trpc-vue-element-plus": "file:..",
    "nodemon": "2.0.19",
    "ts-node": "10.8.2",
    "tsutils": "3.21"
  },
  "author": "Yaniv Peretz",
  "license": "MIT"
}
