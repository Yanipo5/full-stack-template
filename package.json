{
  "name": "full-stack-starter",
  "description": "Full stack template based in (server) prisma + trpc and (client) vue + pinia + element-plus",
  "version": "0.0.1",
  "author": "Yaniv Peretz",
  "license": "MIT",
  "workspaces": [
    "server",
    "client"
  ],
  "main": "server/dist/server.js",
  "scripts": {
    "_____DEVELOPMENT_____": "",
    "dev-client": "npm --prefix client run dev",
    "dev-server": "npm --prefix server run dev",
    "compose": "docker-compose up",
    "init": "npm ci --workspaces",
    "_____DOCKER_____": "",
    "docker-build": "sh docker.build.sh",
    "docker-run-locally": "docker run -p 8080:8080 --env-file .env %npm_package_name%:%npm_package_version%",
    "docker-run-analyze": "docker run -it %npm_package_name%:%npm_package_version% /bin/sh",
    "_____TEST_____": "",
    "test-api": "",
    "test-e2e": "",
    "_____OTHERS_____": "",
    "start": "node server/dist/server.js",
    "build": "npm run build --workspaces"
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "typescript": "^4.8.4"
  }
}
